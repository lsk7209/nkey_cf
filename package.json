{
  "name": "naver-keyword-finder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npx @cloudflare/next-on-pages",
    "start": "next start",
    "lint": "next lint",
    "cf:dev": "wrangler pages dev .vercel/output/static",
    "cf:deploy": "wrangler pages deploy .vercel/output/static --project-name=$PAGES_PROJECT_NAME",
    "d1:migrate": "wrangler d1 migrations apply $D1_DB",
    "d1:console": "wrangler d1 execute $D1_DB --command \"SELECT 1;\""
  },
  "dependencies": {
    "next": "14.0.0",
    "react": "^18",
    "react-dom": "^18",
    "@supabase/supabase-js": "^2.38.0",
    "crypto-js": "^4.2.0",
    "lucide-react": "^0.294.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "@types/crypto-js": "^4.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.0.0",
    "@cloudflare/next-on-pages": "^1.13.4",
    "wrangler": "^3.77.0"
  }
}
